{
  "extends": "solhint:recommended",
  "plugins": ["prettier"],
  "rules": {
    "compiler-version": "off",
    "contract-name-camelcase": "off",
    "func-name-mixedcase": "off",
    "func-visibility": ["warn",{"ignoreConstructors":true}],
    "no-empty-blocks": "off",
    "var-name-mixedcase": "off",
    "prettier/prettier": "error"
  }
}
